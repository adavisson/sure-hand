{
  "manifest_version": 3,
  "name": "Sure Hand",
  "version": "1.0",
  "description": "Verify that you want to raise a hand in google meet meetings.",
  "icons": {
    "16": "images/sure-hand-logo-16.png",
    "32": "images/sure-hand-logo-32.png",
    "48": "images/sure-hand-logo-48.png",
    "128": "images/sure-hand-logo-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://meet.google.com/*"]
    }
  ]
}
